<!DOCTYPE html>
<html>
  <head>
    <%- include('../part/head.ejs') %>
    <%- include('../part/bootstrapCSS.ejs') %>
  </head>
  <body class="on-loading">
    <%- include('../part/bootstrapJS.ejs') %>
    <script src="https://kit.fontawesome.com/071ea1fa85.js" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.js"></script>    
    <%- include('../part/loading.ejs') %>
    
    <%- include('../part/navbar.ejs') %>
    
    <main id="main" role="main" class="container main"> 
      <div class="not-in-channel" id="unchannel">
        <form id="not-in-channel-form">
          <select required class="channels">
            <option value="a" class="channel">a</option>
            <option value="b" class="channel">b</option>
            <option value="c" class="channel">c</option>
            <option value="d" class="channel">d</option>
          </select>
          <br>
          <input class="search-input" type="text" placeholder="search YouTube">
          <button class="btn btn-danger search-btn">Go! <i class="fa fa-youtube"></i></button>
        </form>
        <form id="result">
          <ul>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
            <li><img class="video-result-img" width="168" height="94" src="<%= bot.user.displayAvatarURL() %>"> Result name</li>
          </ul>
        </form>
      </div>
      <div id="channel">
        Hi
      </div>
    </main>

    <%- include('../part/footer.ejs') %>
    <script src="/socket.io/socket.io.js"></script>

    <script>
      var socket = io();
      const status = false;
      
      if (!status) {
        document.getElementById("unchannel").style.display = "block";
        document.getElementById("channel").style.display = "none";
      } else if (status) {
        document.getElementById("unchannel").style.display = "none";
        document.getElementById("channel").style.display = "block";
      }
      
      socket.on("e", function(msg) {
        document.getElementById("h1").innerHTML = msg;
      });
    </script>
  </body>
</html>