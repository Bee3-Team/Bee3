<!DOCTYPE html>
<html>
  <head>
    <%- include('part/head.ejs') %>
    <%- include('part/bootstrapCSS.ejs') %>
  </head>
  <body>
    
    <%- include('part/navbar.ejs') %>
    
    <main id="main" role="main" class="container main">
      <div class="jumbotron jumbotron-fluid">
        <div class="container">
          <div class="jumbo-img">
            <img class="jumbotron-image animate-img" src="<%= bot.user.displayAvatarURL() %>">
          </div>
          <h1 class="display-4">Powerful bot</h1>
          <p class="lead">Auto Mod, Music, Leveling, and much more!</p>
        </div>
      </div>
      <br>
      <br>
      <br>
      <br>
      <div class="status">
        <span class="status-title">status.beee.cf</span>
        <div class="status-card">
          <span class="s-title">Were are online in</span>
          <span class="s-pil"><i class="fas fa-server"></i> <%= bot.guilds.cache.size %></span>
        </div>
        <div class="status-card">
          <span class="s-title">Were are chatting in</span>
          <span class="s-pil"><i class="fas fa-hashtag"></i> <%= bot.channels.cache.size %></span>
        </div> 
        <div class="status-card">
          <span class="s-title">Were are playling with</span>
          <span class="s-pil"><i class="fas fa-users"></i> <%= bot.users.cache.size %></span>
        </div>         
      </div>
    </main>

    <%- include('part/footer.ejs') %>
    <%- include('part/bootstrapJS.ejs') %>
    <script src="https://kit.fontawesome.com/071ea1fa85.js" crossorigin="anonymous"></script>
  </body>
</html>